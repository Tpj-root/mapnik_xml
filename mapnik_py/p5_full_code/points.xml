<?xml version="1.0" encoding="utf-8"?>
<Map background-color='lightgreen'>
  <Style name="style">
    <Rule>
      <PointSymbolizer/>
<TextSymbolizer size="10" fill="black" face-name="DejaVu Serif Book" allow-overlap="true" dy="10" dx="10" wrap-width="1200">

  [label]
</TextSymbolizer>
    </Rule>
  </Style>
  <Layer name="layer">
    <StyleName>style</StyleName>
    <Datasource>
      <Parameter name="type">csv</Parameter>
<Parameter name="inline">
wkt,label
"POINT(0 0)","#ifndef KINEMATICS_H"
"POINT(0 1)","#define KINEMATICS_H"
"POINT(0 2)","0123456789 0123456789 0123456789 0123456789 0123456789 0123456789 0123456789 0123456789 0123456789 0123456789 "
</Parameter>
    </Datasource>
  </Layer>
</Map>
